{
  "@class": "com.tiffanytimbric.fsm.FiniteStateMachine",
  "name": "Trade FSM",
  "dataItem": "fsm_dataItem",
  "ignoreUnknownEvents": true,
  "initialState": {
    "@class": "com.tiffanytimbric.fsm.State",
    "name": "Proposed",
    "dataItem": "proposed_state_dataItem",
    "transitions": [
      {
        "@class": "com.tiffanytimbric.fsm.Transition",
        "event": {
          "@class": "com.tiffanytimbric.fsm.Event",
          "name": "Agree",
          "dataArg": "agree_event_dataArg"
        },
        "toState": {
          "@class": "com.tiffanytimbric.fsm.State",
          "name": "Partially_Agreed",
          "dataItem": "partially_agreed_state_dataItem",
          "transitions": [
            {
              "@class": "com.tiffanytimbric.fsm.Transition",
              "event": {
                "@class": "com.tiffanytimbric.fsm.Event",
                "name": "Agree",
                "dataArg": "agree_event_dataArg"
              },
              "toState": {
                "@class": "com.tiffanytimbric.fsm.State",
                "name": "Agreed",
                "dataItem": "agreed_state_dataItem",
                "transitions": [
                  {
                    "@class": "com.tiffanytimbric.fsm.Transition",
                    "event": {
                      "@class": "com.tiffanytimbric.fsm.Event",
                      "name": "Receive",
                      "dataArg": "receive_event_dataArg"
                    },
                    "toState": {
                      "@class": "com.tiffanytimbric.fsm.State",
                      "name": "Partially_Received",
                      "dataItem": "partially_received_state_dataitem"
                    },
                    "toStateName": null
                  }
                ]
              },
              "toStateName": null
            },
            {
              "@class": "com.tiffanytimbric.fsm.Transition",
              "event": {
                "@class": "com.tiffanytimbric.fsm.Event",
                "name": "Decline",
                "dataArg": "decline_event_dataArg"
              },
              "toState": {
                "@class": "com.tiffanytimbric.fsm.State",
                "name": "Declined",
                "dataItem": "declined_state_dataItem"
              }
            }
          ]
        },
        "toStateName": null
      },
      {
        "@class": "com.tiffanytimbric.fsm.Transition",
        "event": {
          "@class": "com.tiffanytimbric.fsm.Event",
          "name": "Decline",
          "dataArg": "decline_event_dataArg"
        },
        "toState": {
          "@class": "com.tiffanytimbric.fsm.State",
          "name": "Declined",
          "dataItem": "declined_state_dataItem"
        }
      }
    ]
  }
}
